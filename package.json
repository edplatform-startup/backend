{
  "name": "edtech-backend-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Minimal Node.js API for reading courses from Supabase",
  "main": "src/server.js",
  "scripts": {
    "prebuild": "node -e \"console.log('prebuild ok')\"",
    "build": "node -e \"console.log('No build step required for Express')\"",
    "start": "node src/server.js",
    "dev": "node src/server.js",
    "smoke": "node -e \"import('./src/app.js').then(()=>console.log('OK')).catch(e=>{console.error(e?.message||e);process.exit(1)})\"",
    "test": "node --test",
    "render-build": "npm ci --include=dev --prefer-online --no-audit --no-fund --progress=false --foreground-scripts=false && npm run build",
    "render-start": "npm run start",
    "postdeploy": "node scripts/postdeploy.js || node -e \"console.log('no postdeploy')\""
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.45.4",
    "@temporalio/activity": "^1.13.2",
    "@temporalio/client": "^1.13.2",
    "@temporalio/worker": "^1.13.2",
    "@temporalio/workflow": "^1.13.2",
    "axios": "^1.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "instructor": "^1.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.9.1",
    "string-similarity": "^4.0.4",
    "string-similarity-js": "^2.1.4",
    "uuid": "^13.0.0",
    "yt-search": "^2.13.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "supertest": "^7.1.4"
  }
}
